@*@model IEnumerable<Asp_Net_MVC.Models.Employee>*@
@model IEnumerable<Employee>
@{
    //Layout = "~/Views/Shared/_Layout.cshtml";
    //ViewStart page will do this automatically
    ViewBag.Title = "Employees List";
}
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Department</td>
            </tr>
        }
    </tbody>

</table>
@section Scripts{
    <script src="~/js/CustomScript.js"></script>
}
